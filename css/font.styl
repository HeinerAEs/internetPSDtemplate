@font-face {
    font-family  'icomoon'
    src url('../fonts/icomoon.eot?jzpour')
    src url('../fonts/icomoon.eot?#iefixjzpour') format('embedded-opentype'),
        url('../fonts/icomoon.woff?jzpour') format('woff'),
        url('../fonts/icomoon.ttf?jzpour') format('truetype'),
        url('../fonts/icomoon.svg?jzpour#icomoon') format('svg')
    font-weight  normal
    font-style  normal
}

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-play-circle-fill
  &:before
    content "\e604"

.icon-search
  &:before
    content "\e600"

.icon-play
  &:before
    content "\e601"

.icon-facebook
  &:before
    content "\e605"

.icon-twitter
  transform rotate(180deg)
  &:before
    content "\e602"

.icon-github
  &:before
    content "\e606"

